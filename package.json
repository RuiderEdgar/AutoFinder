		"cors": "2.8.5",
		"dotenv": "16.0.3",
		"express": "4.18.2",
		"express-async-errors": "3.1.1",
		"helmet": "7.0.0",
		"hpp": "0.2.3",
		"http-status-codes": "2.2.0",
		"joi": "17.9.2",
