		"eslint-config-prettier": "8.8.0",
		"nodemon": "2.0.22",
		"prettier": "2.8.8",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.0.4",
		"typescript-transform-paths": "3.4.6"
